<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/app_switch_track_radius"/>
            <solid android:color="@color/ebony_clay_2"/>
            <size android:height="@dimen/app_switch_track_height"/>
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/app_switch_track_radius"/>
            <solid android:color="@color/dark_control_primary_bg_v2"/>
            <size android:height="@dimen/app_switch_track_height"/>
        </shape>
    </item>
</selector>